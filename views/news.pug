extends layout

block content
  h1= title

  form(method="get")
    input(type="text" name="search" value=search)
    input(type="submit" value="Szukaj") 
   
  each item in data
    article 
      h1(class="news_title")=item.title
      p(class='news_description')=item.description
      p(class='news_date')=item.date
    br
